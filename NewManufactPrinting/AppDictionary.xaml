<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:NewManufactPrinting">
    <local:MainWindowViewModel x:Key="mwvm"/>
    <Style x:Key="TextBlockWrappingStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="TextWrapping" Value="Wrap"/>
    </Style>
    <Style x:Key="TextBlockRightAlignStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="TextAlignment" Value="Right"/>
    </Style>
</ResourceDictionary>